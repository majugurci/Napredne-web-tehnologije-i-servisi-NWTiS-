                                    <p:column headerText="#{m.pregledAdresa_tablicaVjetar}">
                                        #{key.windSpeedValue} #{key.windSpeedName} 
                                    </p:column>

                                    <p:column headerText="#{m.pregledAdresa_tablicaVrijemeAžuriranja}">
                                        #{key.lastUpdate} 
                                    </p:column>

                                </p:dataTable>
                            </h:panelGroup>
                        </h:panelGroup>
                    </article>


                    <article>
