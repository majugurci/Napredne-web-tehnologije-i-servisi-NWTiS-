package org.foi.nwtis.majugurci.web.podaci;

/**
 *
 * @author Mario
 * 
 * Pomoćna klasa koja riješava bug u PrimeFaces tablici gdje je nemoguće 
 * selektirati "običan" Java objekt tipa String, već to mora biti neka
 * dodatna klasa. U ovom slučaju svakoj adresi dodan je njezin id.
 */
public class sveAdreseId {
    
    private int id;
    private String adresa;

    public sveAdreseId() {
    }
    
    public sveAdreseId(int id, String adresa) {
        this.id = id;
        this.adresa = adresa;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getAdresa() {
        return adresa;
    }

    public void setAdresa(String adresa) {
        this.adresa = adresa;
    }
    
    
    
}
