package org.foi.nwtis.majugurci.web.podaci;

import java.util.Date;

/**
 *
 * @author Mario
 */
public class MeteoPrognozaSati {

    private String adresa;
    private Date sat;
    private MeteoPodaci prognoza;

    public MeteoPrognozaSati() {
    }

    public MeteoPrognozaSati(String adresa, Date sat, MeteoPodaci prognoza) {
        this.adresa = adresa;
        this.sat = sat;
        this.prognoza = prognoza;
    }

    public String getAdresa() {
        return adresa;
    }

    public void setAdresa(String adresa) {
        this.adresa = adresa;
    }

    public Date getSat() {
        return sat;
    }

    public void setSat(Date sat) {
        this.sat = sat;
    }

    public MeteoPodaci getPrognoza() {
        return prognoza;
    }

    public void setPrognoza(MeteoPodaci prognoza) {
        this.prognoza = prognoza;
    }
}
