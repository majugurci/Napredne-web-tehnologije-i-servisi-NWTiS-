package org.foi.nwtis.majugurci.web.podaci;

/**
 *
 * @author Mario
 */
public class MeteoStanice {
    
    private String name;
    private int type;
    private int status;
    private int id;
    private Lokacija location;
    private Float distanceValue;
    private String distanceUnit;

    public MeteoStanice() {
    }

    public MeteoStanice(String name, int type, int status, int id, Lokacija location, Float distanceValue, String distanceUnit) {
        this.name = name;
        this.type = type;
        this.status = status;
        this.id = id;
        this.location = location;
        this.distanceValue = distanceValue;
        this.distanceUnit = distanceUnit;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public Lokacija getLocation() {
        return location;
    }

    public void setLocation(Lokacija location) {
        this.location = location;
    }

    public Float getDistanceValue() {
        return distanceValue;
    }

    public void setDistanceValue(Float distanceValue) {
        this.distanceValue = distanceValue;
    }

    public String getDistanceUnit() {
        return distanceUnit;
    }

    public void setDistanceUnit(String distanceUnit) {
        this.distanceUnit = distanceUnit;
    }
    
    
    
}
