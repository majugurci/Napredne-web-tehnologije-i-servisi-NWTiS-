table {
	border: 1px solid #666;
	width: 100%;
	margin: 20px 0 20px 0 !important;
}

th,td {
	padding: 2px 4px 2px 4px !important;
	text-align: left;
	vertical-align: top;
}

thead tr {
	background-color: #fc0;
}

th.sorted {
	background-color: orange;
}

th a,th a:visited {
	color: black;
}

th a:hover {
	text-decoration: none;
	color: black;
	border-bottom: 0;
	
}

th.sorted a,th.sortable a {
	background-position: right;
	display: block;
	width: 100%;
}

th.sortable a {
	background-image: url(../i/displaytag/arrow_off.png);
	background-repeat: no-repeat;
}

th.order1 a {
	background-image: url(../i/displaytag/arrow_down.png);
	background-repeat: no-repeat;
}

th.order2 a {
	background-image: url(../i/displaytag/arrow_up.png);
	background-repeat: no-repeat;
}

tr.odd {
	background-color: #fff
}

tr.tableRowEven,tr.even {
	background-color: #fea
}

div.exportlinks {
	background-color: #eee;
	border: 1px dotted #999;
	padding: 2px 4px 2px 4px;
	margin: 2px 0 10px 0;
	width: 98%;
}
div.exportlinks a:hover {
	text-decoration: none;
	border-bottom: 0;
}
span.export {
	padding: 0 4px 1px 20px;
	display: inline;
	display: inline-block;
	cursor: pointer;
}

span.excel {
	background-image: url(../i/displaytag/page_white_excel.png);
	background-repeat: no-repeat;
}

span.csv {
	background-image: url(../i/displaytag/page_white_csv.png);
	background-repeat: no-repeat;
}

span.xml {
	background-image: url(../i/displaytag/page_white_code.png);
	background-repeat: no-repeat;
}

span.pdf {
	background-image: url(../i/displaytag/page_white_acrobat.png);
	background-repeat: no-repeat;
}

span.rtf {
	background-image: url(../i/displaytag/page_white_rtf.png);
	background-repeat: no-repeat;
}

span.pagebanner {
	background-color: #eee;
	border: 1px dotted #999;
	padding: 2px 4px 2px 4px;
	width: 98%;
	margin-top: 10px;
	display: block;
	border-bottom: none;
}

span.pagelinks {
	background-color: #eee;
	border: 1px dotted #999;
	padding: 2px 4px 2px 4px;
	width: 98%;
	display: block;
	border-top: none;
	margin-bottom: -5px;
}