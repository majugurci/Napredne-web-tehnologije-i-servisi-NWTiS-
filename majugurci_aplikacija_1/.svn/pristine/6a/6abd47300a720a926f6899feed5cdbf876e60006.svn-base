<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>org.foi.nwtis.majugurci.web.slusaci.SlusacAplikacije</listener-class>
    </listener>
    <context-param>
        <param-name>konfiguracijaBaza</param-name>
        <param-value>NWTiS.db.config_1.xml</param-value>
    </context-param>
    <context-param>
        <param-name>konfiguracijaRazno</param-name>
        <param-value>razne_postavke.xml</param-value>
    </context-param>
    <context-param>
        <param-name>cjenik</param-name>
        <param-value>cjenik.xml</param-value>
    </context-param>
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>MeteoPodaciWS</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>MeteoPodaciWS</servlet-name>
        <url-pattern>/MeteoPodaciWS</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
